# bash aliases
alias ls='ls --color=auto'
alias ll='ls -l'

# common typos
alias cd..='cd ..'
alias wpd=pwd
alias emasc=emacs

alias auau='sudo aptitude update && sudo aptitude full-upgrade'
alias dir='echo "sorry, wrong OS"'

alias calc='libreoffice --calc'
alias writer='libreoffice --writer'

alias week='date +%Y-W%W'
